{
  "apiVersion": "kubeless.io/v1beta1",
  "kind": "Function",
  "metadata": {
    "annotations": {
      "function-size": "{{function_size}}"
    },
    "labels": {
      "app": "{{function_name}}"
    },
    "name": "{{function_name}}",
    "namespace": "{{namespace}}"
  },
  "spec": {
    "checksum": "{{checksum}}",
    "deployment": {
      "spec": {
        "template": {
          "spec": {
            "containers": [
              {
                "env": "{{environment_variables}}",
                "name": "{{function_name}}"
              }
            ]
          }
        }
      }
    },
    "deps": "{{function_deps}}",
    "function": "{{function_code}}",
    "function-content-type": "text",
    "handler": "handler.main",
    "horizontalPodAutoscaler": "{{horizontalPodAutoscaler}}",
    "runtime": "nodejs8",
    "service": {
      "ports": {
        "name": "http-function-port",
        "port": "8080",
        "protocol": "TCP",
        "targetPort": "8080"
      },
      "selector": {
        "created-by": "kubeless",
        "function": "{{function_name}}"
      }
    },
    "timeout": ""
  }
}
